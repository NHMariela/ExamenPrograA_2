    <div class="col-md-4">
        @Html.LabelFor(u => u.Marca)
    </div>
    <div class="col-md-8">
        @Html.DisplayFor(u => u.Marca)
    </div>
    <br />
</div>

<div class="row">
    <div class="col-md-4">
        @Html.LabelFor(u => u.Modelo)
    </div>
    <div class="col-md-8">
        @Html.DisplayFor(u => u.Modelo)
    </div>
    <br />
</div>
<input type="button" value="Volver" class="btn btn-outline-success btn-block btn-lg" onclick="document.location.href='@Url.Content("~/Maquinaria/IndexM")'" />
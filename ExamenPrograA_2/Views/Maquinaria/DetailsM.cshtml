    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalle de la Maquina</h2>

<div class="row">
    <div class="col-md-4">
        @Html.LabelFor(u => u.MaquinariaID)
    </div>
    <div class="col-md-8">
        @Html.DisplayFor(u => u.MaquinariaID)
    </div>
